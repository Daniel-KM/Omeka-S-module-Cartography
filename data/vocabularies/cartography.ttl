@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix geo: <http://www.opengis.net/ont/OGC-GeoSPARQL/1.0/> .
@prefix cartography: <http://localhost/ns/cartography/> .

<http://localhost/ns/cartography/> a owl:Ontology ;
    rdfs:label "Cartography" ;
    rdfs:comment "Specific metadata for cartography (to be removed)." .

cartography:uncertainty a rdf:Property ;
    rdfs:label "Uncertainty" ;
    rdfs:comment "Level of uncertainty of a data." ;
